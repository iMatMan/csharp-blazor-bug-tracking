@page "/new-bug"
<h3>Add New Bug</h3>

<EditForm Model="AddBug">
    <InputText @bind-value="@AddBug.Title" placeholder="Enter Title"></InputText>
    <InputText @bind-value="@AddBug.Description" placeholder="Enter Description"></InputText>
    <Component TValue="@AddBug.Priority" placeholder="Enter Priority"></Component>
    <button type="submit">Add Bug</button>
</EditForm>

@code
{
    public Bug AddBug = new Bug();
}
